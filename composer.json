{
    "name": "it-brains/laravel-release-scripts",
    "description": "Laravel Release Scripts, easy to set up one-off execute scripts that will be running during deploy script.",
    "keywords": [
        "laravel",
        "deploy"
    ],
    "homepage": "https://github.com/spatie/laravel-backup",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolay Zhidenko",
            "email": "dragg.ko@gmail.com",
            "homepage": "http://it-brains.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/console": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/filesystem": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
      "psr-4": {
          "ITBrains\\ReleaseScript\\": "src/"
      }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ITBrains\\ReleaseScript\\ServiceProvider"
            ]
        }
    }
}
